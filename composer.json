{
    "name": "drupal-switzerland/drupalmountaincamp_ch",
    "description": "Drupal Mountain Camp",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "drupal-superfish/superfish",
                "version": "2.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/mehrpadin/Superfish-for-Drupal/archive/2.x.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "amazeeio/drupal_integrations": "^0.3.6",
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^3.0",
        "drupal/core": "^8.9",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.0",
        "drupal/entity": "^1.3",
        "drupal/entity_reference_revisions": "^1.6",
        "drupal/field_group": "^3.2",
        "drupal/field_permissions": "^1.0@RC",
        "drupal/focal_point": "^1.0",
        "drupal/honeypot": "^2.0",
        "drupal/link_attributes": "^1.5",
        "drupal/media_entity_generic": "^1.1",
        "drupal/metatag": "^1.7",
        "drupal/page_manager": "^4.0",
        "drupal/panels": "^4.3",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.6",
        "drupal/purge": "^3.0@beta",
        "drupal/redirect": "^1.3",
        "drupal/superfish": "^1.2",
        "drupal/token": "^1.5",
        "drupal/varnish_purge": "^2.1",
        "drupal/views_conditional": "1.*",
        "drupal/webform": "^6.1",
        "drush/drush": "^10.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.1"
    },
    "require-dev": {
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "#2718657 - Drupal\\language\\LanguageNegotiator::updateConfiguration() is passed a NULL": "https://www.drupal.org/files/issues/2019-02-28/language-2718657-17.patch"
            },
            "drupal/media_entity": {
                "#2918172 - Media Entity upgrade -> core fails on absent column revision_uid": "https://www.drupal.org/files/issues/2020-04-24/2918172-47.patch"
            }
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "allowed-packages": [
                "amazeeio/drupal_integrations"
            ],
            "excludes": [
                "sites/development.services.yml",
                "sites/example.settings.local.php"
            ],
            "file-mapping": {
                "[web-root]/robots.txt": false,
                "[web-root]/sites/default/default.services.yml": false,
                "[web-root]/sites/default/default.settings.php": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/.htaccess": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/LICENSE.txt": false,
                "[web-root]/README.txt": false,
                "[web-root]/web.config": false
            },
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
